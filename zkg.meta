[package]
script_dir = scripts
test_command = cd testing && ./btest.sh
summary = Data schema reporting for Zeek logs
description = This package collects metadata for the logs
    Zeek produces in a given installation. The schema data
    are available for export modules, leaving details of the
    representation and export to those modules.
depends =
  zeek >=5.2.0
  
[template]
source = https://github.com/zeek/package-template
version = v3.1.4
zkg_version = 3.0.1-43
features = github-ci,license

[template_vars]
name = logschema
author = Christian Kreibich <christian@corelight.com>
license = bsd-3
